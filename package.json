{
  "name": "webtrees-theme-rural",
  "version": "2.0.0",
  "description": "Rural theme for webtrees",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jon48/webtrees-theme-rural.git"
  },
  "author": "Jonathan Jaubart",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/jon48/webtrees-theme-rural#readme",
  "scripts": {
    "webtrees": "cross-env process.env.section=webtrees NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env process.env.section=rural NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development-webtrees": "npm run webtrees && npm run development",
    "debug": "cross-env process.env.section=rural NODE_ENV=development node --nolazy --inspect-brk=9229 node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env process.env.section=rural NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "npm run webtrees && cross-env process.env.section=rural NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "stylelint": "stylelint \"src/**/*.scss\" --rd"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "laravel-mix-alias": "^1.0.2",
    "laravel-mix-clean": "^0.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-image-inliner": "^4.0.2",
    "postcss-replace": "^1.1.2",
    "postcss-rtl": "^1.7.3",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
